{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "@okta/courage": [
        "../../packages/@okta/courage-dist"
      ],
      "@okta/mocks/*": [
        "../../playground/mocks/*"
      ],
      "@okta/okta-i18n-bundles": [
        "../util/Bundles.ts"
      ],
      "@okta/qtip": [
        "../../packages/@okta/qtip2/dist/jquery.qtip.js"
      ],
      "config/config.json": [
        "../config/config.json"
      ],
      "nls/*": [
        "../../packages/@okta/i18n/src/json/*"
      ],
      "preact/hooks": [
        "./node_modules/preact/hooks"
      ],
      "react": [
        "./node_modules/preact/compat"
      ],
      "react-dom": [
        "./node_modules/preact/compat"
      ],
      "src/*": [
        "./src/*"
      ],
      "util/*": [
        "../util/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "strict": true,
    "target": "ES5"
  },
  "extends": "../../tsconfig.json",
  "files": [
    "global.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
