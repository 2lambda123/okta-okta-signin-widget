{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "allowJs": true,
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "skipLibCheck": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "checkJs": false,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "config/config.json": ["../config/config.json"],
      "nls/*": ["../../packages/@okta/i18n/src/json/*"],
      "okta-i18n-bundles": ["../util/Bundles.ts"],
      "okta": ["../../packages/@okta/courage-dist"],
      "qtip": ["../../packages/@okta/qtip2/dist/jquery.qtip.js"],
      "util/*": ["../util/*"],
      "preact/hooks": ["./node_modules/preact/hooks"],
      "react": [
        "node_modules/preact/compat"
      ],
      "react-dom": [
        "node_modules/preact/compat"
      ]
    }
  },
}
